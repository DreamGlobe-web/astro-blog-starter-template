{
// wrangler.json
  "name": "astro17",
  "compatibility_date": "2025-04-01",
  "compatibility_flags": ["nodejs_compat"],
  "main": "./dist/_worker.js/index.js",
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS"
  },
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true
}
{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "routes": [
    {
      "pattern": "dreamglobe.com",
      "custom_domain": true
    }
  ]
}
